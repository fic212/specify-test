{
  "repository": "@superbet/design-tokens",
  "head": "specifyrc-json",
  "base": "main",
  "version": "2",
  "rules": [
    {
      "name": "Superbet colors",
      "path": "./output/superbet-colors",
      "parsers": [
        {
          "name": "to-style-dictionary",
          "options": {
            "formatName": "snakeCase",
            "formatTokens": {
              "colorFormat": {
                "format": "hex8"
              }
            }
          }
        }
      ]
    }
  ]
}
